;; ============================================================================
;; Test Suite: Arithmetic Operations
;; Description: Tests for basic arithmetic operations
;; ============================================================================

;; Test: Addition
(+ 1 2)           ;; Expected: 3
(+ 10 20 30)      ;; Expected: 60
(+ ~5 5)          ;; Expected: 0

;; Test: Subtraction
(- 10 5)          ;; Expected: 5
(- 100 30 20)     ;; Expected: 50
(- 0 42)          ;; Expected: -42

;; Test: Multiplication
(* 3 4)           ;; Expected: 12
(* 2 3 4)         ;; Expected: 24
(* ~2 5)          ;; Expected: -10

;; Test: Division
(/ 10 2)          ;; Expected: 5
(/ 100 10 2)      ;; Expected: 5
(/ ~20 4)         ;; Expected: -5

;; Test: Modulo
(% 10 3)          ;; Expected: 1
(% 17 5)          ;; Expected: 2
(% 100 7)         ;; Expected: 2

;; Test: Nested arithmetic
(+ (* 2 3) (/ 10 2))      ;; Expected: 11
(- (* 4 5) (+ 3 2))       ;; Expected: 15
(/ (+ 10 20) (- 10 5))    ;; Expected: 6
