;; ============================================================================
;; Test Suite: Conditional Expressions
;; Description: Tests for if, cond, and, or expressions
;; ============================================================================

;; Test: Simple if expressions
(if #t 1 2)                    ;; Expected: 1
(if #f 1 2)                    ;; Expected: 2
(if (> 5 3) "yes" "no")        ;; Expected: "yes"
(if (< 5 3) "yes" "no")        ;; Expected: "no"

;; Test: Nested if expressions
(if #t (if #t 1 2) 3)           ;; Expected: 1
(if #f 1 (if #t 2 3))           ;; Expected: 2
(if (> 10 5) (+ 1 2) (* 3 4))  ;; Expected: 3

;; Test: And expressions
(and #t #t)                    ;; Expected: #t
(and #t #f)                    ;; Expected: #f
(and #f #t)                    ;; Expected: #f
(and (> 5 3) (< 2 4))          ;; Expected: #t

;; Test: Or expressions
(or #t #f)                    ;; Expected: #t
(or #f #t)                    ;; Expected: #t
(or #f #f)                    ;; Expected: #f
(or (> 5 10) (< 2 4))         ;; Expected: #t

;; Test: Cond expressions
(cond ((< 5 3) 1)
    ((> 5 3) 2)
    ((== 5 3) 3))             ;; Expected: 2

(cond ((== 1 2) "first")
    ((== 2 2) "second")
    ((== 3 3) "third"))       ;; Expected: "second"

;; Test: Complex conditions
(if (and (> 10 5) (< 3 7)) "both true" "not both")    ;; Expected: "both true"
(if (or (> 10 5) (< 7 3)) "at least one" "none")     ;; Expected: "at least one"
