;; ============================================================================
;; Test Suite: String Operations
;; Description: Tests for string concatenation and manipulation
;; ============================================================================

;; Test: String concatenation
(@ "Hello" " " "World")       ;; Expected: "Hello World"
(@ "MLisp" "!")               ;; Expected: "MLisp!"
(@ "" "test")                 ;; Expected: "test"

;; Test: String concatenation with variables
(:= first "Hello")
(:= second "World")
(@ first " " second)          ;; Expected: "Hello World"

;; Test: Nested string operations
(@ (@ "A" "B") "C")           ;; Expected: "ABC"
(@ "Start " (@ "Middle" " End"))  ;; Expected: "Start Middle End"
