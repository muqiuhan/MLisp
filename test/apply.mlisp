;; ============================================================================
;; Test Suite: Apply Function
;; Description: Tests for the apply function
;; ============================================================================

;; Test: Apply with built-in function
(define nums (quote (1 2 3 4 5)))
(apply + nums)                   ;; Expected: 15

;; Test: Apply with lambda
(define multiply (lambda (x y) (* x y)))
(define args (quote (6 7)))
(apply multiply args)            ;; Expected: 42

;; Test: Apply with variable number of arguments
(define list-of-nums (quote (10 20 30)))
(apply + list-of-nums)           ;; Expected: 60
