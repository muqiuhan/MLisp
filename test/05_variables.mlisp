;; ============================================================================
;; Test Suite: Variable Definitions
;; Description: Tests for variable definition and lookup
;; ============================================================================

;; Test: Simple variable definition
(define x 42)
x                             ;; Expected: 42

;; Test: Variable redefinition
(define y 10)
y                             ;; Expected: 10
(define y 20)
y                             ;; Expected: 20

;; Test: Variable with expression
(define z (+ 5 3))
z                             ;; Expected: 8

;; Test: Variable referencing another variable
(define a 5)
(define b (* a 2))
b                             ;; Expected: 10

;; Test: Multiple variables
(define num1 10)
(define num2 20)
(define sum (+ num1 num2))
sum                           ;; Expected: 30

;; Test: String variables
(define greeting "Hello")
(define name "World")
greeting                      ;; Expected: "Hello"
name                          ;; Expected: "World"

;; Test: Boolean variables
(define is-true #t)
(define is-false #f)
is-true                       ;; Expected: #t
is-false                      ;; Expected: #f
