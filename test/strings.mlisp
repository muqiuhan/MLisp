;; ============================================================================
;; Test Suite: String Operations
;; Description: Tests for string concatenation and manipulation
;; ============================================================================

;; Test: String concatenation
(list "Hello" " " "World")       ;; Expected: ("Hello" " " "World")
(list "MLisp" "!")               ;; Expected: ("MLisp" "!")
(list "" "test")                 ;; Expected: ("" "test")

;; Test: String concatenation with variables
(define first "Hello")
(define second "World")
(list first " " second)          ;; Expected: ("Hello" " " "World")

;; Test: Nested string operations
(list (list "A" "B") "C")           ;; Expected: (("A" "B") "C")
(list "Start " (list "Middle" " End"))  ;; Expected: ("Start " ("Middle" " End"))
