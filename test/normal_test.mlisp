;; Get all definitions in the current environment
(env)

;; Integer operations
(+ 3 5)
(- 3 5)
(* 3 5)
(/ 3 5)
(mod 3 5)

;; Logical operations
(and #t #f)
(or #f #f)
(if (and #t #f) 3 4)
(> 3 5)
(< 3 5)
(>= 3 5)
(<= 3 5)
(== 3 5)

;; variable definition
(define x 3)

;; cons
(cons 5 6)

;; list
(list 1 2 3 4 5)

;; apply
(apply cons (list 3 4))

;; lambda
(define x (lambda (y) (+ y 1)))

;; function definition
(defun x (y) (+ y 1))

;; application
(x 10)

;; let
(let ((x 10)
     (y 20))
  (+ x y))

;; let*
(let* ((x 10)
      (y x))
  (+ x y))

;; letrec
(letrec ((f (lambda (x) (g (+ x 1))))
      (g (lambda (x) (+ x 3))))
  (f 0))

;; record
(define record-x (record 'x (list (list 'y 1) (list 'z 2))))

;; record-getter
(record-get record-x 'y)
